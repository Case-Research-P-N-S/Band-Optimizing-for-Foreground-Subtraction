import matplotlib.pyplot as plt


def CompareTwoPlots(xAxis, *args, **kwargs):
    fig = plt.figure()
    needlegend = False
    ax = plt.subplot(2, 1, 1)
    for index in [1,2]:
        name = 'plot{}'.format(index)
        
    if "title" in kwargs:
        plt.title(kwargs.get("title"))
    if "xlabel" in kwargs:
        plt.xlabel(kwargs.get("title"))
    if needlegend == True:
        handles, labels = ax.get_legend_handles_labels()
        plt.legend(handles, labels, loc='upper right')
    if "name" in kwargs:
        plt.savefig(kwargs.get("name"))
    else:
        plt.savefig("lineplots.png")
    plt.close(fig)
